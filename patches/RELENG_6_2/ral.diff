+	bus_space_handle_t		sc_sh;
+
+	struct mtx			sc_mtx;
+
+	struct callout			scan_ch;
+	struct callout			rssadapt_ch;
+
+	int				sc_tx_timer;
+
+	struct ieee80211_channel	*sc_curchan;
+
+	uint8_t				rf_rev;
+
+	uint8_t				rfprog;
+	uint8_t				rffreq;
+
+	struct rt2661_tx_ring		txq[4];
+	struct rt2661_tx_ring		mgtq;
+	struct rt2661_rx_ring		rxq;
+
+	uint32_t			rf_regs[4];
+	int8_t				txpow[38];
+
+	struct {
+		uint8_t	reg;
