+static struct ieee80211_node *
+rt2560_node_alloc(struct ieee80211_node_table *nt)
+{
+	struct rt2560_node *rn;
+
+	rn = malloc(sizeof (struct rt2560_node), M_80211_NODE,
+	    M_NOWAIT | M_ZERO);
