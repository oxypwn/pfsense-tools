+	if (error != 0) {
+		device_printf(sc->sc_dev, "could not allocate Tx ring\n");
+		goto fail1;
+	}
+
+	error = rt2560_alloc_tx_ring(sc, &sc->atimq, RT2560_ATIM_RING_COUNT);
+	if (error != 0) {
