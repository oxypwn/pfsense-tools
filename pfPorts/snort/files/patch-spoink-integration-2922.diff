+    SpoAlertPfData *data = (SpoAlertPfData *)arg;
+    DEBUG_WRAP(DebugMessage(DEBUG_LOG,"AlertPfRestart\n"););
+    
+    s2c_parse_free_wl(&data->head);
+    fclose(data->wlfile);
+    close(data->fd);
+    
+    free(data);
+}
