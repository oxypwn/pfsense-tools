#!/bin/sh
#
# $FreeBSD: ports/sysutils/lcdproc/files/lcdexec.in,v 1.2 2010/03/27 00:14:57 dougb Exp $
#
# PROVIDE: lcdexec
# REQUIRE: LCDd
#
# DO NOT CHANGE THESE DEFAULT VALUES HERE
# SET THEM IN THE /etc/rc.conf FILE
#

. /etc/rc.subr

name="lcdexec"
rcvar=`set_rcvar`

load_rc_config $name

: ${lcdexec_enable="NO"}
: ${lcdexec_conf="%%PREFIX%%/etc/lcdexec.conf"}
: ${lcdexec_flags="-c ${lcdexec_conf}"}

required_files=${lcdexec_conf}
command="%%PREFIX%%/bin/lcdexec"

run_rc_command "$1"
